<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="NFLogPlugin" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug x64">
				<Option output="$(SolutionDir)/_Out/Debug/NFLogPlugin_d" prefix_auto="0" extension_auto="1" />
				<Option object_output=".objs/Debug" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-std=c++11" />
					<Add option="-g" />
					<Add option="-fPIC" />
					<Add option="-D_DEBUG" />
					<Add option="-D_USRDLL" />
					<Add option="-DELPP_NO_DEFAULT_LOG_FILE" />
					<Add directory="$(SolutionDir)" />
					<Add directory="$(SolutionDir)/Dependencies/" />
					<Add directory="$(SolutionDir)/Dependencies/Theron/Include/" />
				</Compiler>
				<Linker>
					<Add library="$(SolutionDir)/_Out/Debug/NFCore_d.so" />
					<Add directory="$(SolutionDir)/_Out/Debug/" />
					<Add directory="$(SolutionDir)/lib/Debug/" />
				</Linker>
				<Environment>
					<Variable name="SolutionDir" value="../../" />
				</Environment>
			</Target>
			<Target title="Release x64">
				<Option output="$(SolutionDir)/_Out/Release/NFLogPlugin" prefix_auto="0" extension_auto="1" />
				<Option object_output=".objs/Release" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-std=c++11" />
					<Add option="-fPIC" />
					<Add option="-DNDEBUG" />
					<Add option="-D_USRDLL" />
					<Add option="-DELPP_NO_DEFAULT_LOG_FILE" />
					<Add directory="$(SolutionDir)" />
					<Add directory="$(SolutionDir)/Dependencies/" />
					<Add directory="$(SolutionDir)/Dependencies/Theron/Include/" />
				</Compiler>
				<Linker>
					<Add library="$(SolutionDir)/_Out/Release/NFCore.so" />
					<Add directory="$(SolutionDir)/_Out/Release/" />
					<Add directory="$(SolutionDir)/lib/Release/" />
				</Linker>
				<Environment>
					<Variable name="SolutionDir" value="../../" />
				</Environment>
			</Target>
		</Build>
<Unit filename="NFCLogModule.cpp" />
<Unit filename="dllmain.cpp" />
<Unit filename="NFLogPlugin.cpp" />
<Unit filename="NFCLogModule.h" />
<Unit filename="NFLogPlugin.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
